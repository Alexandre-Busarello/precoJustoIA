{
  "name": "analisador-acoes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/build-with-migrations.js",
    "build:vercel": "node scripts/build-with-migrations.js",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "fetch:data": "tsx scripts/fetch-data.ts",
    "fetch:data:free": "tsx scripts/fetch-data.ts 1",
    "fetch:data:paid": "tsx scripts/fetch-data.ts 10",
    "fetch:data:ward": "tsx scripts/fetch-data-ward.ts",
    "fetch:ward:local": "node scripts/run-fetch-ward.js --local",
    "fetch:ward:remote": "node scripts/run-fetch-ward.js",
    "fetch:ward:reset": "node scripts/run-fetch-ward.js --reset",
    "update:profiles": "tsx scripts/update-company-profiles.ts",
    "update:profiles:dry": "tsx scripts/update-company-profiles.ts --dry-run",
    "debug:api": "curl -s http://localhost:3000/api/debug-ranking-history | jq ."
  },
  "dependencies": {
