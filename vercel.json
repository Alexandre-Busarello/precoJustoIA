{
  "crons": [
    {
      "path": "/api/cron/fetch-ward",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/monitor-assets",
      "schedule": "0 */6 * * *"
    }
  ],
  "functions": {
    "src/app/api/**": {
      "memory": 1024
    },
    "src/app/api/fetch-ward-data/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/fetch-ward/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/monitor-assets/route.ts": {
      "maxDuration": 300
    }
  },
  "env": {
    "VERCEL": "1"
  }
}